@{
    ViewData["Title"] = "Serviços CIMOB";
    Layout = "~/Views/Shared/_LayoutWhite.cshtml";
}

<link href="~/css/cimob.css" rel="stylesheet" />

<div class="cimob">
    <div class="header">
        <img src="~/images/home/1.svg">
        <img src="~/images/home/2.svg">
        <img src="~/images/home/3.svg">
        <img src="~/images/home/4.svg">
        <img src="~/images/home/5.svg">
        <img src="~/images/home/6.svg">
        <img src="~/images/home/7.svg">
        <img src="~/images/home/8.svg">
        <img src="~/images/home/9.svg">
    </div>

    <div class="button-wrapper">
        <div class="container-fluid ">
            <div class="col-md-3 col-md-offset-3">
                <a asp-controller="Editais" asp-action="Index" class="menu-button">Editais</a>
            </div>
            <div class="col-md-3">
                <a asp-controller="ServicosCIMOB" asp-action="Candidaturas" class="menu-button">Candidaturas</a>
            </div>
        </div>

        <div class="container-fluid" style="margin-top: 20px">
            <div class="col-md-4 col-md-offset-4">
                <a asp-controller="EscolasParceiras" asp-action="Index" class="menu-button">Escolas Parceiras</a>
            </div>
        </div>
